# Copy this file to .env and provide the actual credentials/secrets.
DATABASE_URL=postgresql://username:password@hostname:5432/database
DEFAULT_USER_ID=00000000-0000-0000-0000-000000000000

# Firebase configuration
FIREBASE_SECRET_ID=firebase-service-account
FIREBASE_CREDENTIALS_FILE=/absolute/path/to/firebase-service-account.json
FIREBASE_SERVICE_ACCOUNT_JSON={"type":"service_account"}
FIREBASE_ROOT_COLLECTION=app_usage_events
FIREBASE_EVENTS_SUBCOLLECTION=events

# AWS configuration for Secrets Manager lookups
AWS_REGION=us-east-1

# Optional processing hints
USER_ID_LIMIT=50
